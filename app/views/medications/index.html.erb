<div class="container mt-5">
  <h1>Medications#index</h1>
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-sm-8">
        <div id="medications">
          <%= @results.each do |result| %>
            <h4><%= result.searchable.active_substance %></h4>
            <p><%= result.searchable.name_and_concentration %></p>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
